    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/poke-fall-api/bin/Debug/net7.0/poke-fall-api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/poke-fall-api",
            "console": "internalConsole",
            "stopAtEntry": false,
            "requireExactSource": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT":"Development",
                "ASPNETCORE_URLS":"http://localhost:5028"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
        }
    ]
